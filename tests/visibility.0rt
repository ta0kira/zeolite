// Author: Kevin P. Barry [ta0kira@gmail.com]

testcase "public types in private deps stay hidden" {
  success Test$run()
}

// Also defined differently in visibility/internal/internal.0rp.
concrete Internal {
  @type create () -> (Internal)
  @value get () -> (String)
}

define Internal {
  create () {
    return Internal{ }
  }

  get () {
    return "message"
  }
}

define Test {
  run () {
    String value1 <- Internal$create().get()
    if (value1 != "message") {
      fail(value1)
    }
    Int value2 <- Getter$getValue()
    if (value2 != 1) {
      fail(value2)
    }
  }
}

concrete Test {
  @type run () -> ()
}
