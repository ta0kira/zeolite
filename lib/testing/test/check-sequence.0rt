testcase "CheckSequence:equalsQuiet different" {
  failure TestChecker
  require "10.+40"
  exclude "20.+50"
  exclude "30.+60"
}

unittest test {
  ValueList<Int> actual <- ValueList<Int>.new().append(10).append(20).append(30)
  ValueList<Int> expected <- ValueList<Int>.new().append(40).append(50).append(60)
  \ actual `Matches:with` CheckSequence:equalsQuiet(expected)
}


