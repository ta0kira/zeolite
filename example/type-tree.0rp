concrete TypeTree {
  @type new () -> (TypeTree)
  @value set<#x> (TypeKey<#x>,#x) -> (TypeTree)
  @value remove (TypeKey<any>) -> (TypeTree)
  @value get<#x> (TypeKey<#x>) -> (optional #x)
}

concrete TypeKey<|#x> {
  defines LessThan<TypeKey<any>>
  defines Equals<TypeKey<any>>

  @type new () -> (TypeKey<#x>)
}
